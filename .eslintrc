{
  "extends": "airbnb",
  "env": {
    "mocha": true,
    "node": true,
  },
  "rules": {
    "quotes": [2, "double"],

    // disabled
    "arrow-body-style": [0],
    "max-len": [0],
    "no-param-reassign": [0],
    "no-return-assign": [0],
    "no-underscore-dangle": [0],
    "object-curly-spacing": [0],
  },
}
